<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul>
      <% productsList.map(product => { %>
        <li>
          <h2><%= product.name %></h2>
          <% product.tags.map((tag, index) => { %>
            <p>TAG <%= index + 1 %>: <%= tag %></p> 
          <% }) %>
        </li>
      <% }) %>
    </ul>
  </body>
</html>
